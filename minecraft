echo "| | | | | ||||||||||||||||||||||||||||||| | | | | |"
echo "|_|_|_|_|_| | | | | | | | | | | | | | | |_|_|_|_|_|"
echo "| | | | | |_|_|_|_|_|_|_|_|_|_|_|_|_|_|_| | | | | |"
echo "| | | | | | | | | | | | | | | | | | | | | | | | | |"
echo "|_|_|_|_|_|_|_|_|_|_|_|_|_|_|_|_|_|_|_|_|_|_|_|_|_|"
 
java -jar ~/games/Minecraft.jar
